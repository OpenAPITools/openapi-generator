play {
  filters.headers.contentSecurityPolicy = null

  http {
    secret.key = "changeme"
    {{#handleExceptions}}
    errorHandlers="openapitools.ErrorHandler"
    {{/handleExceptions}}
  }

  assets {
    path = "/public"
    urlPrefix = "/assets"
  }
}
