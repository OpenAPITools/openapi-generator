#Articus\PathHandler\Attribute\PluginManager:
#  factories:
#  invokables:

#Articus\PathHandler\Producer\PluginManager:
#  factories:
#  invokables:

Laminas\ServiceManager\AbstractFactory\ConfigAbstractFactory:
{{#apiInfo}}
{{#apis}}
{{#operations}}
  {{package}}\{{classname}}: []
{{/operations}}
{{/apis}}
{{/apiInfo}}
