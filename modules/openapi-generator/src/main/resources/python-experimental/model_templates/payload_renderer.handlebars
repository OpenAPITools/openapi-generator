{{#or isNumber isUnboundedInteger}}
    {{value}}
{{/or}}
{{#if isBoolean}}
    {{#if value}}
    True
    {{else}}
    False
    {{/if}}
{{/if}}
{{#if isNull}}
    None
{{/if}}
{{#if isString}}
    "{{value}}"
{{/if}}
{{#if isArray}}
    [
    {{#each value}}
    {{> model_templates/payload_renderer }}
    {{/each}}
    ]
{{/if}}
{{#if isMap}}
    {
    {{#each value}}
        "{{@key}}":
        {{#with this}}
    {{> model_templates/payload_renderer }},
        {{/with}}
    {{/each}}
    }
{{/if}}