{
  "name": "{{npmName}}",
  "version": "{{npmVersion}}",
  "description": "OpenAPI client for {{npmName}}",
  "author": "OpenAPI-Generator Contributors",
  "repository": {
    "type": "git",
    "url": "https://{{gitHost}}/{{gitUserId}}/{{gitRepoId}}.git"
  },
  "keywords": [
    "fetch",
    "typescript",
    "openapi-client",
    "openapi-generator"
  ],
  "license": "{{licenseName}}",
  "main": "./dist/index.js",
  {{#supportsES6}}
  "type": "module",
  "module": "./dist/index.js",
  {{/supportsES6}}
  {{^supportsES6}}
  "type": "commonjs",
  {{/supportsES6}}
  "exports": {
    ".": {
      "import": "./dist/index.js",
      {{^supportsES6}}
      "require": "./dist/index.js",
      {{/supportsES6}}
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build"
  },
  "dependencies": {
    {{#frameworks}}
      {{#fetch-api}}
        {{#platforms}}
          {{#node}}
    "undici": "^7.18.2",
          {{/node}}
          {{#browser}}
    "whatwg-fetch": "^3.0.0",
          {{/browser}}
        {{/platforms}}
      {{/fetch-api}}
      {{#jquery}}
    "@types/jquery": "^3.3.29",
    "jquery": "^3.4.1",
      {{/jquery}}
    {{/frameworks}}
    {{#platforms}}
      {{#node}}
    "@types/node": "^20.17.10",
    "form-data": "^4.0.4",
      {{/node}}
    {{/platforms}}
    {{#useRxJS}}
    "rxjs": "^6.4.0",
    {{/useRxJS}}
    {{#useInversify}}
    "inversify": "^6.0.1",
    {{/useInversify}}
    "es6-promise": "^4.2.4"
  },
  "devDependencies": {
    "typescript": "^{{typescriptMajorVersion}}.0"
  }{{#npmRepository}},{{/npmRepository}}
{{#npmRepository}}
  "publishConfig":{
    "registry":"{{npmRepository}}"
  }
{{/npmRepository}}
}
