{{#useNose}}
[nosetests]
logging-clear-handlers=true
verbosity=2
randomize=true
exe=true
with-coverage=true
cover-package={{{packageName}}}
cover-erase=true

{{/useNose}}
[flake8]
max-line-length=99

[mypy]
ignore_missing_imports=True
show_error_codes=True

## Uncomment the following if you need more context around errors
#show_error_context=True
#pretty=True
#error_summary=True
