  built_value: '>=8.4.0 <9.0.0'
  built_collection: '>=5.1.1 <6.0.0'
{{/useBuiltValue}}
{{#useEquatable}}
  equatable: '^2.0.7'
{{/useEquatable}}
{{#useJsonSerializable}}
  json_annotation: '^4.9.0'
{{/useJsonSerializable}}
{{#useDateLibTimeMachine}}
  time_machine: ^0.9.17
{{/useDateLibTimeMachine}}

dev_dependencies:
{{#useBuiltValue}}
  built_value_generator: '>=8.4.0 <9.0.0'
