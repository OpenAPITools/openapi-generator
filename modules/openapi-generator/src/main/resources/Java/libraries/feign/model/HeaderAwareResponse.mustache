package {{modelPackage}};

import java.util.Map;

public class HeaderAwareResponse<T>{

  private Map<String,Object> headers;

  private T body;

  public HeaderAwareResponse(Map<String, Object> headers, T body) {
    this.headers = headers;
    this.body = body;
  }

  public T getBody(){
    return body;
  }

  public Map<String,Object> getHeaders(){
    return headers;
  }
}