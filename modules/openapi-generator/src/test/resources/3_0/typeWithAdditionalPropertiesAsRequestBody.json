		}
	},
	"servers": [
		{
			"url": ""
		}
	],
	"tags": [
		{
			"name": "MyClass"
		}
	],
	"paths": {
		"/samplefunction": {
			"post": {
				"tags": [
					"SampleResource"
				],
				"summary": "Sample function",
				"operationId": "sampleFunction",
				"parameters": [],
				"requestBody": {
					"description": "Request body with a schema that has additionalProperties",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SampleSchema"
							}
						}
					},
					"required": true
				},
				"responses": {
					"201": {
						"description": "Sample response"
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"SampleSchema": {
				"type": "object",
				"properties": {
					"prop1": {
						"type": "string"
					},
					"prop2": {
						"type": "string"
					}
				},
				"additionalProperties": true
			}
		}
	}
}