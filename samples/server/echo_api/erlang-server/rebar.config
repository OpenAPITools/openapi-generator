{minimum_otp_vsn, "27"}.

{deps, [
    {cowboy, "2.12.0"},
    {ranch, "2.1.0"},
    {jesse, "1.8.1"}
]}.

{dialyzer,
 [{plt_extra_apps, [cowboy, cowlib, ranch, jesse]},
  {warnings, [no_match, no_unused, missing_return, unknown]}
]}.

{xref_checks,
 [undefined_function_calls, deprecated_function_calls, deprecated_functions]}.
