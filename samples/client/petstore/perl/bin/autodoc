#!/usr/bin/perl
package MyAutodoc;
use FindBin;
use File::Spec;
use lib File::Spec->catdir($FindBin::Bin, '..', 'lib');

use Moose;

with ('WWW::SwaggerClient::Role', 'WWW::SwaggerClient::Role::AutoDoc');

sub auth_setup_handler {}

package main;

my $api = MyAutodoc->new;

print $api->autodoc;

exit(0);
